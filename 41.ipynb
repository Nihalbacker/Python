   "source": [
    "#List leap years within a range\n",
    "y=int(input(\"Enter the year to be started with:\"))\n",
    "n=int(input(\"Enter the year to be ended with:\"))\n",
    "print(\"Leap year from\",y,\"to\",n,\"is:\")\n",
    "for n in range(y,n):\n",
    "      if n%4==0 and (n%100!=0 or n%400==0):\n",
    "        print(n)\n",
    "      "
   ]
  }
