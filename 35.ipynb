    "#calculate numbers and digits in a string\n",
    "n=input(\"Enter the string:\")\n",
    "a=b=0\n",
    "for d in n:\n",
    "     if d.isdigit():\n",
    "        a=a+1    \n",
    "     elif d.isalpha():\n",
    "        b=b+1\n",
    "     else:\n",
    "        pass\n",
    "print(\"letters\",b)\n",
    "print(\"digits\",a)\n"
   ]
